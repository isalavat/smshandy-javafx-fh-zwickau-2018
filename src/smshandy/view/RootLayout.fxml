<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>

<BorderPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="smshandy.view.RootOverviewController">
   <top>
      <HBox blendMode="DARKEN" prefHeight="20.0" prefWidth="700.0" style="-fx-background-color: lightgrey; -fx-border-color: darkgrey;" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="network" mnemonicParsing="false" onAction="#changeToNetwork" text="Network" />
            <Button fx:id="message" mnemonicParsing="false" onAction="#changeToMessage" text="Message" />
         </children>
      </HBox>
   </top>
</BorderPane>
